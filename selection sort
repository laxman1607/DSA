#include<iostream>
using namespace std;
int main(){
    int arr[5]={8,7,2,4,3};
    int n=5;
    for(int i=0;i<n;i++){
        
        for(int j=i+1;j<n;j++){
            if(arr[i]>arr[j]){
               swap(arr[i],arr[j]);
                
            }
            else{
                 swap(arr[i],arr[i]);
                    
                }
                
        }
       
        
    }
    for(int i=0;i<5;i++){
        cout<<arr[i];
    }
}
